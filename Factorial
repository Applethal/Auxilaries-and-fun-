def functionfactorial(x): 
    if x == 0: 
        return 1
    elif x == 1:
        return 1
    y = x - 1 
    
    while y > 1:
         factorial = x * y
         y -= 1 
         x = factorial
         
    return factorial 


factorial = int(input())

print("factorial of your number is",factorial)
